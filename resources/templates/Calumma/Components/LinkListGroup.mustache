<div class="list-group">
	{{#links}}
	<a id="{{id}}" href="{{href}}" class="list-group-item {{#class}} {{.}} {{/class}} {{#active}} active{{/active}}" {{#target}}target="{{.}}"{{/target}}  title="{{title}}" {{#data}}data-{{key}}="{{value}}"{{/data}}>
		<i class="{{^iconClass}} bs-icon-linklist-default {{/iconClass}} {{#iconClass}} {{.}} {{/iconClass}}" style="width:0;"></i>
		<span style="width:auto;">{{text}}</span>
		{{#badge}}
		<span class="badge">{{badge}}</span>
		{{/badge}}
		<span class="pull-right-container">
			<i class="bs-chevron pull-right" style="width:0;"></i>
		</span>
	</a>
	{{/links}}
</div>